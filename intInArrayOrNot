public class Main{
    public static void main(String[] args){
        int[] marks = {12, 10, 23, 87};
        int num = 12;
        boolean isInArray = false;
        for(int element:marks){
            if(num==element){
                isInArray = true;
                break;
            }
        }
        if(isInArray){
            System.out.println("value is present");
        }
        else{
            System.out.println("value not present");
        }
    }
}
