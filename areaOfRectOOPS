class Rectangle{
    int length;
    int breadth;
  public int getLength(){
      return length;
  }
  public int getBreadth(){
      return breadth;
  }
  public int area(){
      return length*breadth;
  }
  public int perimeter(){
      return 2*(length+breadth);
  }
}

public class Methods{
    public static void main(String[] args){
        Rectangle rect = new Rectangle();
        rect.length = 12;
        rect.breadth = 5;
        System.out.println("The length of rectangle is " + rect.getLength());
        System.out.println("The breadth of rectangle is " + rect.getBreadth());
        System.out.println("The area of rectangle is " + rect.area());
        System.out.println("The perimeter of rectangle is " + rect.perimeter());
    }
}
